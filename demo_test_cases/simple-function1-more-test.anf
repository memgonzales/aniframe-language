func make_list(x: Number, y: Number) returns List:
    return [x, y]

# ======================

func factorial(x: Number) returns Number:
    if x >= 1:
        return factorial(x - 1) * x
    
    return 1

func fib(x: Number) returns Number:
    if x <= 1:
        return x 
    else:
        return fib(x - 1) + fib(x - 2)


list = make_list(100, 200)
torso = draw_bird_torso(list[0], list[1])

wing = flap_wing(270)

