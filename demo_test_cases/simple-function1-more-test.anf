func draw_bird_torso(x: Number, y: Number) returns Object:
    x = (-1 * (x * 1 + 1 - 1)) * -1
    return rectangle(x, y, 200, 100)

func draw_bird_wing(x: Number) returns Object:
    y = x - 20
    z = (x - 10) * 0.5
    a = x - 10 * 2
    b = its_the_same(50 * 2 * 2 + 2 - 2)
    c = sqrt(10000)
    return triangle(x, y, z, a, b, c)


func its_the_same(num: Number) returns Number:
    return num * 1

func its_the_same(num: Number) returns Number:
    return num * 2

func flap_wing(num: Number) returns Object:
    wing = draw_bird_wing(num)

    wing.move(100, 0, 100, 1000)
    wing.turnCC(180, 100, 1000)

    return wing

